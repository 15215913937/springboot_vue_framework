<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqn.library.mapper.RenheCollectMapper">
    <select id="findPage" resultType="com.sqn.library.entity.RenheCollect">
        SELECT *
        from `renhe_collect`
        <where>
            <if test="code!=null and code !=''">
                code like concat('%',#{code},'%')
            </if>
            <if test="bedId!=null and bedId !=''">
                and bed_id like concat('%',#{bedId},'%')
            </if>
            <if test="mat!=null and mat !=''">
                and mat like concat('%',#{mat},'%')
            </if>
            <if test="batch!=null and batch !=''">
                and batch = #{batch}
            </if>
            <if test="coefficient!=null and coefficient !=''">
                and coefficient = #{coefficient}
            </if>
            <if test="status!=null and status !=''">
                and status = #{status}
            </if>
        </where>
        order by create_time desc
    </select>
</mapper>
