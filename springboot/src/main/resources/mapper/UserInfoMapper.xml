<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqn.library.mapper.UserInfoMapper">
    <select id="findPage" resultType="com.sqn.library.entity.UserInfo">
        SELECT *
        from `user_info`
        <where>
            <if test="name!=null and name !=''">
                name like concat('%',#{name},'%')
            </if>
        </where>
        order by name desc
    </select>
</mapper>
